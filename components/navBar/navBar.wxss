.nav-bar .ios {
    --height: 44px;
    /* 4*2+32 */
    --right: 97px;
    /* 10+87 */
    --navBarExtendHeight: 4px;
    box-sizing: border-box;
}

.nav-bar .android {
    --height: 48px;
    /* 8*2+32 */
    --right: 97px;
    /* 10+87 */
    --navBarExtendHeight: 4px;
    box-sizing: border-box;
}

.nav-bar__inner {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 5001;
    height: var(--height);
    display: flex;
    align-items: center;
    padding-right: var(--right);
    width: 100%;
    padding-bottom: var(--navBarExtendHeight);
}

.nav-bar__left {
    position: relative;
    display: flex;
    align-items: center;
}

.nav-bar__buttons {
    height: 100%;
    width: 100%;
    display: flex;
    align-items: center;
    border-radius: 16px;
    border: 1rpx solid rgba(234, 234, 234, 1);
    position: relative;
}

.nav-bar__buttons::after {
    position: absolute;
    content: '';
    width: 1rpx;
    height: 18.4px;
    background: rgba(204, 204, 204, 0.6);
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
}

.nav-bar__button {
    width: 50%;
    height: 100%;
    display: flex;
    font-size: 12px;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 1em 2em;
}

.nav-bar__center {
    flex: 1;
    font-size: 17px;
    line-height: 17px;
    font-weight: bold;
    text-align: center;
}

.nav-bar__center.white {
    color: #fff;
}

